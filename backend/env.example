PORT=4000
# OpenRouter API Configuration
# Get your API key from https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_api_key_here
# Free-tier models: google/gemini-flash-1.5:free, meta-llama/llama-3.2-3b-instruct:free, meta-llama/llama-3.1-8b-instruct:free
AI_MODEL=meta-llama/llama-3.2-3b-instruct:free
# Optional: HTTP Referer header (for analytics/attribution)
OPENROUTER_HTTP_REFERER=http://localhost:4000
# Optional: X-Title header (for analytics)
OPENROUTER_X_TITLE=Assimetria Challenge
# Optional: tweak generation behavior/timeouts
OPENROUTER_TIMEOUT_MS=30000
OPENROUTER_MAX_TOKENS=500
OPENROUTER_TEMPERATURE=0.7
# Cron schedule for automatic article generation (daily at 3 AM)
CRON_SCHEDULE=0 3 * * *
# CORS allowed origin
ALLOWED_ORIGIN=http://localhost:4173




